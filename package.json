{
  "name": "imageprocessingapi",
  "version": "1.0.0",
  "description": "An endpoint for resizing images.",
  "main": "app.js",
  "scripts": {
    "build": "npx tsc",
    "dev": "tsc --watch",
    "lint": "eslint --ext  .ts --fix .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "clean-code": "npm run lint && npm run format",
    "jasmine": "jasmine",
    "test-build": "npm run build && npm run jasmine",
    "start-server": "node ./dist/app.js",
    "start-dev-server": "nodemon ./src/app.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ahmed121221/image-resizer.git"
  },
  "keywords": [
    "resize",
    "images",
    "api",
    "typescript",
    "express",
    "node"
  ],
  "author": "Ahmed Ragab",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ahmed121221/image-resizer/issues"
  },
  "homepage": "https://github.com/Ahmed121221/image-resizer#readme",
  "devDependencies": {
    "@types/jasmine": "^3.10.3",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "jasmine": "^4.0.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^6.0.0",
    "@types/request": "^2.48.8",
    "@types/sharp": "^0.29.5",
    "express": "^4.17.2",
    "express-rate-limit": "^6.2.1",
    "jasmine-spec-reporter": "^7.0.0",
    "sharp": "^0.30.1"
  }
}
